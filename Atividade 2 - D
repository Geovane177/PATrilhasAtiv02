using System;

namespace Atividade_2___D
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("Digite um números: ");
			int numero1 = Convert.ToInt32(Console.ReadLine());
		
			
			int quadrado = numero1 * numero1;
			
			Console.WriteLine("o numero "+ numero1 +" ao quadrado é: "+ quadrado);
			
			Console.Write("Pressione para continuar . . . ");
			Console.ReadKey(true);
		}
	}
}
